{
	"version": "0.2.0",
	"configurations": [
		{
            "name": "Launch NodeJS",
            "type": "node",
            // Workspace relative or absolute path to the program.
            "program": "${workspaceRoot}/build/server/server.js",
            "stopOnEntry": true,
            "args": [],
			"cwd": "${workspaceRoot}",
			"preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": false,
            "sourceMaps": true,
            "outDir": null        
		}, 
		{
			"name": "Attach",
			"type": "chrome",
			"request": "attach",
			"sourceMaps": true,
			"port": 9222
		}
	]
}